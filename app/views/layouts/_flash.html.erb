<% flash.each do |key, value| %>
  <% if key == "alert" %>
    <% key="danger" %>
  <% elsif key == "notice" %>
     <% key="info"%>
  <% end %> 
  <div class="alert alert-<%= key %> alert-dismissible">
    <button type="button" class="close" data-dismiss="alert">Ã—</button>
    <%= value %>
  </div>
<% end %>